<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.quiz.lesson05.dao.WeatherhistoryDAO">
 	<select id="selectweatherhistoryList" resultType="com.quiz.lesson05.model.Weatherhistory">
 		SELECT
 			`id`
			, `date`
			, `weather`
			, `temperatures`
			, `precipitation`
			, `microDust`
			, `windSpeed`
			, `createdAt`
			, `updatedAt`
		FROM 
			`weatherHistory`
 	</select>
 	
 	<insert id="insertWeatherhistory" parameterType="map">
 		INSERT INTO `weatherhistory`
		(	
			 `date`
			, `weather`
			, `temperatures`
			, `precipitation`
			, `microDust`
			, `windSpeed`
			, `createdAt`
			, `updatedAt`
 		)
 		VALUES
 		(	
 			#{date}
 			,#{weather}
 			,#{temperatures}
 			,#{precipitation}
 			,#{microDust}
 			,#{windSpeed}
 			,NOW()
 			,NOW()
 		)
 		
 	</insert>
 </mapper>