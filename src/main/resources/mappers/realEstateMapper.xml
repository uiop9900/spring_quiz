<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.quiz.lesson03.dao.RealEstateDAO">
 	<select id="selectrealEstate" parameterType="int" resultType="com.quiz.lesson03.model.realEstate">
 		SELECT
			`id`
			, `realtorId`
			, `address` 
			, `area` 
			, `type` 
			, `price` 
			, `rentPrice`
			, `createdAt`
			, `updatedAt`
 		FROM
 			`real_estate`
		WHERE
			`id` = #{id}
 	</select>
 </mapper>
 
 <mapper namespace="com.quiz.lesson03.dao.RealEstateDAO">
 	<select id="seletrealEstate" parameterType="Integer" resultType="com.quiz.lesson03.model.realEstate">
 		SELECT
			`id`
			, `realtorId`
			, `address` 
			, `area` 
			, `type` 
			, `price` 
			, `rentPrice`
			, `createdAt`
			, `updatedAt`
 		FROM
 			`real_estate`
		WHERE
			`rentPrice` < #{rentPrice}
 	</select>
 </mapper>